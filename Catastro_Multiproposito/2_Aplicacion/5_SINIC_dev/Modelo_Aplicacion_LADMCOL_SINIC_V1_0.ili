      /** Indica la clasificación del tipo de punto de acuerdo a sus características.
       */
      !!@ ili2db.dispName = "Tipo de punto"
      PuntoTipo (EXTENDED) : MANDATORY Modelo_Aplicacion_LADMCOL_SINIC_V1_0.SINIC_PuntoTipo;
      /** Indica si el tipo de punto corresponde a una construcción o servidumbre de tránsito.
       */
      !!@ ili2db.dispName = "Tipo de punto de levantamiento"
      Tipo_Punto_Levantamiento : MANDATORY Modelo_Aplicacion_LADMCOL_SINIC_V1_0.SINIC_PuntoLevTipo;
      /** Indica si el punto es fotoidentificable.
       */
      !!@ ili2db.dispName = "Fotoidentificación"
      Fotoidentificacion : Modelo_Aplicacion_LADMCOL_SINIC_V1_0.SINIC_FotoidentificacionTipo;
      /** Corresponde a la exactitud horizontal del punto levantamiento.
       */
      !!@ ili2db.dispName = "Exactitud horizontal"
      Exactitud_Horizontal : MANDATORY 0.000 .. 10.000 [INTERLIS.m];
      /** Corresponde a la exactitud vertical del punto levantamiento
       */
      !!@ ili2db.dispName = "Exactitud vertical"
      Exactitud_Vertical : 0.000 .. 10.000 [INTERLIS.m];
      !!@ name = "ValidacionValoresPuntoTipo"
      MANDATORY CONSTRAINT PuntoTipo <> #Control ;
      MANDATORY CONSTRAINT PuntoTipo <> #Otro ;
    END SINIC_PuntoLevantamiento;

    ASSOCIATION sinic_ph_predio =
      lc_predio -- {1} SINIC_Predio;
      lc_datos_ph -- {0..1} SINIC_DatosPHCondominio;
    END sinic_ph_predio;

    ASSOCIATION sinic_predio_copropiedad =
      predio -- {0..*} SINIC_Predio;
      copropiedad -<> {0..1} SINIC_Predio;
      coeficiente : LADM_COL_V3_0.LADM_Nucleo.Fraccion;
    END sinic_predio_copropiedad;

    /** Tipo de unidad espacial que permite la representación de una servidumbre de paso asociada a una COL_BAUnit.
     */
    !!@ ili2db.dispName = "Servidumbre de Tránsito"
    CLASS SINIC_ServidumbreTransito
    EXTENDS LADM_COL_V3_0.LADM_Nucleo.COL_UnidadEspacial =
      /** Área total de la servidumbre.
       */
      !!@ ili2db.dispName = "Área de la servidumbre"
      Area_Servidumbre : MANDATORY 0.0 .. 99999999999999.9 [LADM_COL_V3_0.m2];
    END SINIC_ServidumbreTransito;

    /** Es cada conjunto de materiales consolidados dentro de un predio que tiene unas características específicas en cuanto a elementos constitutivos físicos y usos de la misma.
     */
    !!@ ili2db.dispName = "Unidad de Construcción"
    CLASS SINIC_UnidadConstruccion
    EXTENDS LADM_COL_V3_0.LADM_Nucleo.COL_UnidadEspacial =
      /** Identificado de la unidad de construcción, su codificación puede ser por letras del abecedario.
       */
      !!@ ili2db.dispName = "Identificador"
      Identificador : MANDATORY TEXT*20;
      /** Código de identificación de la unidad de construcción.
       */
      !!@ ili2db.dispName = "Código de la unidad de construcción"
      Codigo_Unidad_Construccion : TEXT*30;
      /** Indica el tipo de dominio de la unidad de construcción: común y privado.
       */
      !!@ ili2db.dispName = "Tipo de dominio"
      Tipo_Dominio : Modelo_Aplicacion_LADMCOL_SINIC_V1_0.SINIC_DominioConstruccionTipo;
      /** Indica si la unidad de construcción es residencial, comercial, industrial o anexo.
       */
      !!@ ili2db.dispName = "Tipo de unidad de construcción"
      Tipo_Unidad_Construccion : MANDATORY Modelo_Aplicacion_LADMCOL_SINIC_V1_0.SINIC_UnidadConstruccionTipo;
      /** Indica el tipo de planta donde se ubica la unidad de construcción.
       */
      !!@ ili2db.dispName = "Tipo de planta"
      Tipo_Planta : MANDATORY Modelo_Aplicacion_LADMCOL_SINIC_V1_0.SINIC_ConstruccionPlantaTipo;
      /** Indica numéricamente la ubicación del predio de acuerdo al tipo de planta.
       */
      !!@ ili2db.dispName = "Planta ubicación"
      Planta_Ubicacion : MANDATORY 0 .. 500;
      /** Número total de  habitaciones en la unidad de construcción.
       */
      !!@ ili2db.dispName = "Total de habitaciones"
      Total_Habitaciones : 0 .. 999999;
      /** Número total de baños en la unidad de construcción.
       */
      !!@ ili2db.dispName = "Total de baños"
      Total_Banios : 0 .. 999999;
      /** Número total de locales en la unidad de construcción.
       */
      !!@ ili2db.dispName = "Total de locales"
      Total_Locales : 0 .. 999999;
      /** Actividad que se desarrolla en una unidad de construcción.
       */
      !!@ ili2db.dispName = "Uso"
      Uso : Modelo_Aplicacion_LADMCOL_SINIC_V1_0.SINIC_UsoUConsTipo;
      /** Año de construcción de la unidad de construcción.
       */
      !!@ ili2db.dispName = "Año de construcción"
      Anio_Construccion : 1512 .. 2500;
      /** Avalúo catastral de la unidad de construcción.
       */
      !!@ ili2db.dispName = "Avalúo de la unidad de construcción"
      Avaluo_Unidad_Construccion : LADM_COL_V3_0.LADM_Nucleo.Peso;
      /** Área total construida en la unidad de construcción.
       */
      !!@ ili2db.dispName = "Área construida"
      Area_Construida : MANDATORY 0.0 .. 99999999999999.9 [LADM_COL_V3_0.m2];
      /** Área total privada de la unidad de construcción para los predios en régimen de propiedad horizontal.
       */
      !!@ ili2db.dispName = "Área privada construida"
      Area_Privada_Construida : 0.0 .. 99999999999999.9 [LADM_COL_V3_0.m2];
      /** Altura total de la unidad de construcción.
       */
      !!@ ili2db.dispName = "Altura"
      Altura : 1.00 .. 1000.00 [INTERLIS.m];
      /** Observaciones generales respecto de la unidad de construcción.
       */
      !!@ ili2db.dispName = "Observaciones"
      Observaciones : TEXT;
    END SINIC_UnidadConstruccion;

    ASSOCIATION sinic_construccion_unidadconstruccion =
      lc_unidadconstruccion -- {0..*} SINIC_UnidadConstruccion;
      lc_construccion -<> {1} SINIC_Construccion;
    END sinic_construccion_unidadconstruccion;

  END Sistema_Nacional_Informacion_Catastral;

END Modelo_Aplicacion_LADMCOL_SINIC_V1_0.
