       */
      !!@ ili2db.dispName = "Interrelacionado"
      Interrelacionado : MANDATORY BOOLEAN;
      /** Indica si el Código Homologado se encuentra inscrito en folio de matrícula inmobiliaria. Cuando el Código Homologado se encuentra inscrito en folio de matrícula inmobiliaria pasa a denominarse NUPRE.
       */
      !!@ ili2db.dispName = "Código homologado en FMI"
      Codigo_Homologado_FMI : MANDATORY BOOLEAN;
