      Tipo (EXTENDED) : Modelo_Aplicacion_LADMCOL_SINIC_V1_0.SINIC_PredioTipo;
      /** Caracterización temática del predio.
       */
      !!@ ili2db.dispName = "Condición del predio"
      Condicion_Predio : MANDATORY Modelo_Aplicacion_LADMCOL_SINIC_V1_0.SINIC_CondicionPredioTipo;
      /** Dirección del predio.
       */
      !!@ ili2db.dispName = "Dirección"
      Direccion : BAG {1..*} OF LADM_COL_V3_0.LADM_Nucleo.ExtDireccion;
      /** Es la clasificación para fines estadísticos que se da a cada inmueble en su conjunto–terreno, construcciones o edificaciones-, en el momento de la identificación predial de conformidad con la actividad predominante que en él se desarrolle.
       */
      !!@ ili2db.dispName = "Destinación económica"
      Destinacion_Economica : MANDATORY Modelo_Aplicacion_LADMCOL_SINIC_V1_0.SINIC_DestinacionEconomicaTipo;
      /** Clasificación del suelo según el instrumento de planeación vigente.
       */
      !!@ ili2db.dispName = "Clase del suelo"
      Clase_Suelo : MANDATORY Modelo_Aplicacion_LADMCOL_SINIC_V1_0.SINIC_ClaseSueloTipo;
      /** Categoría del suelo según el instrumento de planeación vigente.
       */
      !!@ ili2db.dispName = "Categoría del suelo"
      Categoria_Suelo : Modelo_Aplicacion_LADMCOL_SINIC_V1_0.SINIC_CategoriaSueloTipo;
      !!@ name = "ValidacionValoresPredioTipo"
      UNIQUE Codigo_Homologado; 
      MANDATORY CONSTRAINT Tipo <> #Ordenamiento_Territorial ;
      MANDATORY CONSTRAINT Tipo <> #Servicios_Publicos ;
      MANDATORY CONSTRAINT Tipo <> #Reservas_Naturales ;
      MANDATORY CONSTRAINT Tipo <> #Parques_Naturales ;
      MANDATORY CONSTRAINT Tipo <> #Amenazas_Riesgos ;
      MANDATORY CONSTRAINT Tipo <> #Servidumbre ;
      MANDATORY CONSTRAINT Tipo <> #Superficies_Agua ;
      MANDATORY CONSTRAINT Tipo <> #Transporte ;
    END SINIC_Predio;

