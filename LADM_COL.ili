        STRUCTURE ExtArchivo =
      /** Fecha en la que ha sido aceptado el documento.
       */
      Fecha_Aceptacion : INTERLIS.XMLDate;
      /** Datos que contiene el documento.
       */
      Datos : CharacterString;
      /** Última fecha de extracción del documento.
       */
